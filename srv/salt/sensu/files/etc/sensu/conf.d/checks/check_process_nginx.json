{
    "checks": {
        "process_nginx": {
            "command": "/etc/sensu/plugins/check-process.pl -c status -p nginx",
            "wiki": "http://wiki.something/for/check/failure",
            "interval": 60,
            "occurrences": 5,            
            "handlers": [ 
                "default",
                "slack",
                "mailer"
            ],
            "subscribers": [
                "nginx",
                "webservice"
            ]
        }
    }
}
