{
    "api.version": "v1",
    "source": {
        "name": "nginx_access_log",
        "sourceType": "LocalFile",
        "category": "Application/nginx/access_log",
        "pathExpression": "/var/log/nginx/access_log",
        "useAutolineMatching": false,
        "multilineProcessingEnabled": false,
        "targetCpu": 50,
        "automaticDateParsing": true,
        "forceTimeZone": false,
        "cutoffRelativeTime": "-1d",
        "blacklist":[]
    }
}
